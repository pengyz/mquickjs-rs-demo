singleton TestTypes {
    fn echoBool(v: bool) -> bool;
    fn echoInt(v: int) -> int;
    fn echoDouble(v: double) -> double;
    fn echoString(v: string) -> string;

    fn echoStringNullable(v: string?) -> string?;
    fn echoIntNullable(v: int?) -> int?;

    fn echoAny(v: any) -> any;

    // TODO(V1): union is part of V1 compliance, but ridl-tool currently cannot
    // generate Rust types for union. Enable these after union support lands.
    fn echoStringOrInt(v: string | int) -> string | int;
    fn echoStringOrIntNullable(v: string | int | null) -> (string | int)?;
}
