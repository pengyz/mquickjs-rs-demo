module test_module_basic@1.0

// Phase 0: minimal module-mode coverage.
fn mping() -> int;

class MFoo {
    // Phase 1: instance JS-only fields
    var x: int = 1;
    var y: string = "foo";

    // Phase 1: proto var
    proto var px: int = 100;

    fn add(a: int, b: int) -> int;

    // C1: basic param/return types coverage
    fn echo_bool(v: bool) -> bool;
    fn echo_int(v: int) -> int;
    fn echo_double(v: double) -> double;
    fn echo_string(v: string) -> string;
    // NOTE: class methods with any return need dedicated glue support; keep param-only here.
    fn echo_any(v: any) -> void;

    // Phase 1: cross-class construction
    fn make_bar(v: int) -> MBar;

    // Phase 2: cross-class parameter passing
    fn use_bar(b: MBar) -> int;

    // MC2: same-name method across classes should not pollute prototypes
    fn get_v() -> int;
}

class MBar {
    var v: int = 0;
    fn get_v() -> int;
}
