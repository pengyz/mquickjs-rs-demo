// mquickjs标准库平台相关功能定义
// 这些功能将通过全局注册方式注册到全局对象

// 日期相关
class Date {
    fn now() -> f64;
}

// 垃圾回收函数
fn gc();

// 性能相关函数
fn performance_now() -> f32;

// 加载脚本函数
fn load(filename: string) -> object;

// 定时器相关函数
fn setTimeout(cb: callback(success: bool), delay: i32) -> i32;
fn clearTimeout(timerId: i32);

// 控制台对象
singleton console {
    fn log(content: string);
    fn error(content: string);
    readonly property enabled: bool;
}

